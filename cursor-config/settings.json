      "border-radius": "12px !important"
      // "overflow": "hidden" /* 둥글게 자른 영역 안 보이게 */
    },
    /* 에디터 배경 이미지 적용 시 둥근 모서리 유지 */
    ".monaco-editor .overflow-guard": {
      "border-radius": "12px !important"
    },
    ".monaco-editor .cursors-layer > .cursor": {
      "height": "16px !important"
    }
