    /* 에디터 전체 */
    ".monaco-editor .margin, .monaco-editor .glyph-margin": {
      "width": "30px !important" /* 기본 40px 이상인 경우, 좁힘 */
    },
    /* 코드 시작 위치 padding 조정 */
    ".monaco-editor .lines-content": {
      "padding-left": "20px !important" /* 기본 8px 정도 -> 4px로 축소 */
    },
    /* 에디터 전체 테두리 둥글게 */
